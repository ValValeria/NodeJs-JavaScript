<script>

let ofd=document.querySelectorAll('.cicket');
    Array.from(document.querySelectorAll('.dod>a')).forEach(elem=>{
        elem.addEventListener('click',(e)=>{
             e.preventDefault();
             $attr=elem.getAttribute('href');
             for( let elem of ofd){
                 elem.style.display='block'
                 if($attr!=='#tab2' && !elem.matches($attr)){
                     elem.style.display="none";
                 }

             }
        })
    })

 setTimeout(()=>{
     for(let elem of ofd){
     elem.onmouseenter=()=>{
       let i= elem.lastElementChild.lastElementChild;
       i.style.display='flex';
       i.style.animationName="shad";
     }
     elem.onmouseleave=()=>{
        elem.lastElementChild.lastElementChild.style.display='none';
     }
 }},0)   
